    <script>

        catform.addEventListener('submit', (e) => {
         document.getElementById("catError").innerHTML="";
      
       let messages = [];
       var format = /[!@#$%^&*()_+\-=\[\]{};':"\\|,.<>\/?1234567890]+/;

             
     let catName=document.getElementById("product_name");

       if (catName.value === '' || catName.value == null) {
         document.getElementById("catError").innerHTML="Category is required";
         messages.push('Cat is required');
       }
       else if (format.test(catName.value)){
            document.getElementById("catError").innerHTML="Category should not contain specical character or number";
            messages.push('Name should not contain specical character');
            } 

        
       if (messages.length > 0) {
         e.preventDefault()
       }
     })
 
     </script>






