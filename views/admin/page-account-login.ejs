    <%- include('../partials/adminNav.ejs') %>



        
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Users List</h2>
                </div>
                <div>
                    <!-- <a href="#" class="btn btn-light rounded font-md">Export</a>
                    <a href="#" class="btn btn-light rounded  font-md">Import</a>
                    <a href="#" class="btn btn-primary btn-sm rounded">Create new</a> -->
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row align-items-center">
                        <div class="col-md-2 col-6">
                            <select class="form-select">
                                <option selected>Status</option>
                                <option>Blocked</option>
                                <option>Active</option>
                                <option>Show all</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <article class="itemlist">
                        <% userList.forEach(function(item){ %>
                        <div class="row align-items-center">
                            

                            <div class="col-lg-2 col-sm-4 col-8 flex-grow-1 col-name">
                                <a href="#"> 
                                    <span>
                                        <%= item.name %>
                                    </span>
                                </a>
                            </div>
                            <div class="col-lg-2 col-sm-4 col-8 flex-grow-1 col-name"> 
                                <span>
                                    <%= item.email %>
                                </span> 
                            </div>
                            
                           <% if(item.userBlocked){  %>
                                <div class="col-lg-2 col-sm-2 col-4 col-status">
                                    <a href="/admin/page-account-login.html/un_block/<%= item._id %>">
                                    <span class="badge rounded-pill alert-warning">
                                        Blocked
                                    </span>
                                    </a>
                                </div>
                         <%   }else{     %>

                                <div class="col-lg-2 col-sm-2 col-4 col-status">
                                    <a href="/admin/page-account-login.html/block/<%= item._id %>">
                                    <span class="badge rounded-pill alert-success">
                                        Active
                                    </span>
                                    </a>
                                </div>

                          <%  }  %>

                            
                            
                            <div class="col-lg-2 col-sm-4 col-8 flex-grow-1 col-name my-3">
                                <span>
                                    <%= item.phone %>
                                </span>
                            </div>
                            <!-- <div class="col-lg-2 col-sm-2 col-4 col-action text-end">
                                <a href="#" class="btn btn-md font-sm rounded btn-brand mt-10 ">
                                    <i class="material-icons md-edit"></i>
                                </a>
                            
                                <a href="#" class="btn btn-md font-sm btn-light rounded mt-10 ">
                                    <i class="material-icons md-delete_forever"></i>
                                </a>
                            </div> -->
                            <br>
                      
                        </div> <!-- row .// -->
                        <% })  %>
                    </article> <!-- itemlist  .// -->
                    
                    
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
